<div class= "container text-center jumbotron">
	<div>
		<h1> Edit your house details</h1>

<%= form_for @house do |f| %>
<p>
  <%= f.label :Location  %><br>
  <%= f.text_field :location  %>
</p>
<p>
  <%= f.label :Rent %><br>
  <%= f.text_field :Rent %>
</p>
<p>
  <%= f.label :Address %><br>
  <%= f.text_area :Address %>
</p>
<p>
  <%= f.label :Description %><br>
  <%= f.text_area :Description %>
</p>
<p>
  <%= f.submit %>
</p>

<% end %>
<%= link_to "Back to houses listing", houses_path %>
</div>
</div>

