<h1> Log in page </h1>

<%= form_for :session, url: login_path do |f| %>
<p>
	<%= f.label :email%> <br/>
	<%= f.email_field :email%>
</p>
<p>
	<%= f.label :password%> <br/>
	<%= f.password_field :password%>
</p>
<p>
	<%= f.submit "Log in" %>
</p>
<% end %>
<p> New User? <%= link_to 'Sign Up', signup_path %> </p>
<%= link_to 'Back to houses List', houses_path %>
